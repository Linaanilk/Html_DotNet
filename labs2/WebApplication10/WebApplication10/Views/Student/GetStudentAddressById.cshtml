@model WebApplication10.Models.Address

<div class="container">
    <h1 class="text-center">Student Address Detail</h1>
    <table class="table">
        <tr>
            <th>House Number</th>
            <td>@Model.HouseNumber</td>
        </tr>
        <tr>
            <th>City</th>
            <td>@Model.City</td>
        </tr>
        <tr>
            <th>Country</th>
            <td>@Model.Country</td>
        </tr>
    </table>
</div>