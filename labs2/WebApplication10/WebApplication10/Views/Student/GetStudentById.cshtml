@model WebApplication10.Models.Student

<div class="container">
    <h1 class="text-center">Student Address Detail</h1>
    <table class="table">
        <tr>
            <th>Id</th>
            <td>@Model.Id</td>
        </tr>
        <tr>
            <th>Name</th>
            <td>@Model.Name</td>
        </tr>
        <tr>
            <th>Email</th>
            <td>@Model.Email</td>
        </tr>
     
    </table>
</div>