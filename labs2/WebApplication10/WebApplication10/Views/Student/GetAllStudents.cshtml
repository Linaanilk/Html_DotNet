@model IEnumerable<WebApplication10.Models.Student>

    <div class="container">
        <h1>All Students</h1>

        <table class="table">
            <thead>
                <tr>
                    <th>Id</th>
                <th>Name</th>
                <th>Email</th>
                <th>Address</th>
                </tr>
            </thead>
            <tbody>
                @foreach(var student in Model)
            {
             <tr>
                 <td>@student.Id</td>
                    <td>@student.Name</td>
                    <td>@student.Email</td>
                    <td>
                        @Html.ActionLink("View","GetStudentById",student.Id)
                    </td>
             </tr>   
            }
           
            </tbody>
        </table>
    </div>